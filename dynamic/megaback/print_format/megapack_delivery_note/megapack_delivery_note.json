 "doctype": "Print Format",
 "font": "Default",
 "html": "<!DOCTYPE html>\n<html>\n<head>\n     <style>\n        @page { margin: 0; }\n        body { margin: 0; padding: 0; }\n        .header, .footer { width: 100%; text-align: center; }\n        .header img, .footer img { width: 100%; }\n        .content { direction: rtl; width: 100%; margin-bottom: 50px; margin-top: 50px; }\n        .item_tr{ border: 1px dashed black; }\n        .items { width: 100%; border-collapse: collapse; border: 1px solid #999999; border-bottom: 2px solid #000000;}\n        td { text-align: center;}\n        .align-center{ text-align:center; }\n        .align-right{ text-align:right; }\n        .align-left{ text-align:left; }\n        .fs16{ font-size: 16px; }\n        .fs14{ font-size: 14px; }\n        .fs12{ font-size: 12px; }\n        .bold{ font-weight: bold; }\n    </style>\n</head>\n<body>\n\n<div class=\"header\">\n    <img class=\"header img\" src=\"/assets/dynamic/images/megaback_header.jpeg\">\n</div>\n\n<div class=\"content\">\n    \n    <h2 style=\"direction: rtl; text-align: center; text-decoration: underline;\"> <strong>\u062a\u0633\u0644\u064a\u0645\u0629 \u0631\u0642\u0645:</strong> {{ doc.name}} </h2>\n    <div class=\"row\" style=\" margin-top: 30px;\">\n        <div class=\"col-5 col-xs-5 align-center fs14 bold\"> \u0627\u0644\u062a\u0627\u0631\u064a\u062e: {{doc.posting_date}}</div>\n        <div class=\"col-2 col-xs-2\"></div>\n        <div class=\"col-5 col-xs-5 fs14 bold\" style=\"text-align:center; \">Print Date \u0627\u0644\u062a\u0627\u0631\u064a\u062e: {{ frappe.utils.now().split()[0] }}</div>\n    </div>\n    <table style=\" margin-top: 30px; width:100%\">\n        <tr>\n            <td style=\"width:50%\"><div class=\"bold align-center fs14\" dir=\"auto\">  \u0627\u0644\u0639\u0645\u064a\u0644 :  {{doc.customer_name}} </div></td>\n            <td style=\"width:50%\"><div class=\"bold align-center fs14\" dir=\"auto\"> \u0627\u0644\u0633\u0627\u0626\u0642 :  {{doc.driver_name or '' }}</div></td>\n        </tr>\n    </table>\n    <table class=\"items\" style=\" margin-top: 30px; height: 50px\">\n        <tr class=\"item_tr\" style=\"border: 2px solid #000000; min-height: 30px;\">\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" > \u0631\u0642\u0645 \u0627\u0644\u0635\u0646\u0641 </div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" > \u0627\u0633\u0645 \u0627\u0644\u0635\u0646\u0641 </div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" > \u0639\u0628\u0648\u0629 </div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" >\u0627\u0644\u0639\u062f\u062f<br>\u0628\u0648\u062d\u062f\u0629<br>\u0627\u0644\u0635\u0646\u0641</div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" >\u0648\u062d\u062f\u0629<br>\u0627\u0644\u0635\u0646\u0641</div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" >\u0627\u0644\u0639\u062f\u062f<br>\u0628\u0648\u062d\u062f\u0629<br>\u0627\u0644\u0645\u062e\u0632\u0646</div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" >\u0648\u062d\u062f\u0629<br>\u0627\u0644\u0645\u062e\u0632\u0646</div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" >\u0627\u0644\u0633\u0639\u0631<br>\u0642\u0628\u0644<br>\u0627\u0644\u062e\u0635\u0645</div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" > \u0627\u062c\u0645\u0627\u0644\u0649<br>\u0627\u0644\u062e\u0635\u0645 </div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" > \u0627\u0644\u0642\u064a\u0645\u0629 </div></td>\n            <td style=\"border: 1px dashed black;\" > <div class=\"bold fs12\" > \u0627\u0644\u0635\u0627\u0641\u0649 </div></td>\n        </tr >\n        {% for item in doc.items %}\n        <tr class=\"item_tr\">\n            {% set uom = get_uom(item.item_code) %}\n            <td>  <div dir=\"auto\" >{{ item.item_code }}</div> </td>\n            <td>  <div dir=\"auto\" style=\"unicode-bidi: bidi-override; word-wrap:break-word; word-break: break-all; white-space: normal;\" >{{ item.item_name }}</div> </td>\n            <td>  <div dir=\"auto\" style=\"unicode-bidi: bidi-override; word-wrap:break-word; word-break: break-all; white-space: normal;\">{{ item.description }}</div> </td>\n            <td>  <div dir=\"auto\" >{%if uom[1] %} {{ item.qty / uom[1] }} {%endif%}</div> </td>\n            <td>  <div dir=\"auto\" >{{ uom[0] }}</div> </td>\n            <td>  <div dir=\"auto\" >{{ item.stock_qty }}</div> </td>\n            <td>  <div dir=\"auto\" >{{ item.stock_uom  }}</div> </td>\n            <td>  <div dir=\"auto\" >{{ item.price_list_rate }}</div> </td>\n            <td>  <div dir=\"auto\" >{{ item.discount_amount * item.qty }}</div> </td>\n            <td>  <div dir=\"auto\" >{{ item.price_list_rate * item.qty }}</div> </td>\n            <td>  <div dir=\"auto\" >{{ item.net_amount }} </div> </td>\n        </tr>\n        {% endfor %}\n    </table>\n    <table style=\" margin-top: 30px; width:100%\">\n         <tr>\n            {%set total = get_total_discount_and_amount(\"Delivery Note\", doc.name) %}\n            <td style=\"width:25%\"><div class=\"bold align-center fs14\" dir=\"rtl\" style=\"unicode-bidi: bidi-override;\"> \u0627\u062c\u0645\u0627\u0644\u0649 \u0627\u0644\u0642\u064a\u0645\u0629: {{ total[0] }} </div></td>\n            <td style=\"width:25%\"><div class=\"bold align-center fs14\" dir=\"auto\" style=\"unicode-bidi: bidi-override;\"> \u062e\u0635\u0645 \u0627\u0644\u0623\u0635\u0646\u0627\u0641: {{ total[1] }} </div></td>\n            <td style=\"width:25%\"><div class=\"bold align-center fs14\" dir=\"auto\" style=\"unicode-bidi: bidi-override;\"> \u062e\u0635\u0645 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629: {{ doc.discount_amount }} </div></td>\n            <td style=\"width:25%\"><div class=\"bold align-center fs14\" dir=\"auto\" style=\"unicode-bidi: bidi-override;\"> \u0627\u0644\u0635\u0627\u0641\u0649: {{ doc.grand_total }}  </div></td>\n        </tr> \n    </table>\n    <table style=\" margin-top: 30px; width:100%\">\n        <tr>\n            {% set curr_balance = get_customer_outstanding_balance(doc.customer_name) %}\n            <td style=\"width:50%\"><div class=\"bold align-center fs14\" dir=\"rtl\" style=\"unicode-bidi: bidi-override;\"> \u0631\u0635\u064a\u062f \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u062d\u0627\u0644\u0649: {{ curr_balance }} </div></td>\n            <td style=\"width:50%\"><div class=\"bold align-center fs14\" dir=\"rtl\" style=\"unicode-bidi: bidi-override;\"> \u0631\u0635\u064a\u062f \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0633\u0627\u0628\u0642: {{ curr_balance - doc.grand_total }} </div></td>\n        </tr>\n    </table>\n    <div class=\"row\" style=\" margin-top: 50px;\">\n        <div class=\"col-12 col-xs-12 align-center fs16 bold\" style=\" padding-left: 100px; text-transform: uppercase;\"> {{ doc.in_words }}</div>\n    </div>\n    <div class=\"row\" style=\" margin-top: 50px;\">\n        <div class=\"col-6 col-xs-6 bold align-center fs16\" style=\"white-space: pre;\"> \u0627\u0644\u0645\u0631\u0627\u062c\u0639\u0629   <span class=\"fs12\">.............................................</span> </div>\n        <div class=\"col-6 col-xs-6 bold align-center fs16\" style=\"white-space: pre;\"> \u0627\u0644\u062d\u0633\u0627\u0628\u0627\u062a   <span class=\"fs12\">.............................................</span> </div>\n    </div>\n</div>\n\n<div class=\"footer\" style=\"margin-top: 30px;\">\n    <img src=\"/assets/dynamic/images/megaback_footer.jpeg\">\n</div>\n\n</body>\n</html>\n",
