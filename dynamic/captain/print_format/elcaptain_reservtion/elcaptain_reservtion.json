 "html": "<!DOCTYPE html>\n  <html>\n    <body>\n    <style>\n{% set customer = frappe.get_doc(\"Customer\", doc.customer) %}\n{% if customer %}\n    {% set customer_primary_address = customer.customer_primary_address %}\n    {% if customer_primary_address %}\n        {% set address = frappe.get_doc(\"Address\", customer_primary_address) %}\n    {% else %}\n        {% set address = none%}\n    {% endif %}\n{% else %}\n    {% set address = none %}\n{% endif %}\n\n            .body {\n            font-family: Arial, sans-serif; /* Choose a font that supports Arabic */\n            direction: rtl; /* Set text direction to right-to-left */\n        }\n            .row {\n                /* display: flex; */\n            }\n            .head, .second {\n                /* flex: 1; */\n                /* display: flex; */\n                font-size: 10px;\n                text-align: center;\n                align-items: center;\n                justify-content: center;\n                min-height: 14px;\n            }\n            .head {\n                font-weight: 300;\n                background-color:#ddd;\n            }\n            .head-col-border{\n                border: 1px solid #333333; \n                /padding: 1px;/\n            }\n            .second-col-border{\n                border: 0.5px solid #555555; \n                /padding: 1px;/\n            }\n            .sections{\n                background-color: #000;\n                color: #fff;\n                width:15%;\n                text-align: center;\n                direction: rtl;\n            }\n            .mt-3{\n                margin-top:1px;\n            }\n            .text-decoration{\n                text-decoration: underline;\n                text-decoration-style: solid;\n            }\n            .checkbox-container {\n                display: flex;\n                align-items: center;\n            }\n            .outer{\n                display: flex;\n                justify-content: center;\n            }\n            .content{\n               /* white-space: pre-line; */\n               font-size: 8px;\n               padding: 5px;\n               direction: rtl;\n               overflow-wrap: break-word;\n               width: 80%;\n            }\n        </style>\n        {% for item in doc.items %}\n        {% set item_doc = frappe.get_doc(\"Item\", item.item_code)%}\n        {% if item.idx == 1 %}\n        <div class=\"container-fluid\" style=\"width:100%; height: 100%;\">\n            <h3 class=\"text-center text-decoration\" style=\"margin-top: 0px;\"> \u0627\u0633\u062a\u0645\u0627\u0631\u0629 \u062d\u062c\u0632</h3>\n            <p class=\"mt-3 sections\"> \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0648\u062d\u062f\u0629</p>\n            <div class=\"container-fluid\" style=\"width:100%\">\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-1 col-1 head head-col-border\">\u0645\u0634\u0631\u0648\u0639</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{item}} </div>\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0643\u0648\u062f \u0627\u0644\u0648\u062d\u062f\u0629</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\"> {{item.item_code}} </div>\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0646\u0648\u0639 \u0627\u0644\u0648\u062d\u062f\u0629</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{item.item_name}}</div>\n                </div>\n                <div class=\"row mt-4\"> \n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0639\u0645\u0627\u0631\u0629</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{item_doc.unit_no}}</div> \n                    <div class=\"col-xs-1 col-1 head head-col-border\">\u0627\u0644\u062f\u0648\u0631</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{item_doc.unit_Floor}}</div>\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0627\u0644\u0646\u0634\u0627\u0637</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{item}}</div>\n                    <div class=\"col-xs-1 col-1 head head-col-border\">\u0627\u0644\u0645\u0633\u0627\u062d\u0629</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{item_doc.unit_area}}</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0627\u0644\u0645\u0633\u0627\u062d\u0629 \u0627\u0644\u062f\u0627\u062e\u0644\u064a\u0629</div>\n                    <div class=\"col-xs-3 col-3 second second-col-border\">{{item_doc.area_indoor}}</div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0627\u0644\u0645\u0633\u0627\u062d\u0629 \u0627\u0644\u062e\u0627\u0631\u062c\u064a\u0629</div>\n                    <div class=\"col-xs-3 col-3 second second-col-border\">{{item_doc.area_outdoor}}</div>\n                    <div class=\"col-xs-1 col-1 second\" style=\"text-align: right; margin-right:5px;\"> \u0645\u062a\u0631 \u062a\u0642\u0631\u064a\u0628\u0627</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-4 col-4 head head-col-border\">\u0642\u064a\u0645\u0629 \u0627\u0644\u0648\u062d\u062f\u0629</div>\n                    <div class=\"col-xs-4 col-4 second second-col-border\">{{doc.total}}</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0641\u0642\u0637 \u0648\u0642\u062f\u0631\u0647</div>\n                    <div class=\"col-xs-8 col-8 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\" >\u0645\u0643\u0627\u0646 \u0645\u062e\u0635\u0635 \u0644\u0627\u0646\u062a\u0638\u0627\u0631\u0627\u0644\u0633\u064a\u0627\u0631\u0629</div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u064a\u0648\u062c\u062f</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">test</div>\n                    <div class=\"col-xs-3 col-3 head head-col-border\"> \u0644\u0627\u064a\u0648\u062c\u062f</div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">test</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-4 col-4 head head-col-border\">\u0628\u0642\u064a\u0645\u0629 \u0627\u0644\u062c\u0631\u0627\u062c</div>\n                    <div class=\"col-xs-4 col-4 second second-col-border\">{{doc.warehouse_amount}}</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0641\u0642\u0637 \u0648\u0642\u062f\u0631\u0647</div>\n                    <div class=\"col-xs-8 col-8 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-4 col-4 head head-col-border\">\u0625\u062c\u0645\u0627\u0644\u0649 \u0642\u064a\u0645\u0629 \u0627\u0644\u0648\u062d\u062f\u0629 \u0634\u0627\u0645\u0644\u0629 \u0642\u064a\u0645\u0629 \u0627\u0644\u062c\u0631\u0627\u062c</div>\n                    <div class=\"col-xs-4 col-4 second second-col-border\">{{doc.grand_total}}</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0641\u0642\u0637 \u0648\u0642\u062f\u0631\u0647</div>\n                    <div class=\"col-xs-8 col-8 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0646\u0633\u0628\u0629 \u0627\u0644\u062e\u0635\u0645</div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">{{doc.additional_discount_percentage}}</div>\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0642\u064a\u0645\u0629 \u0627\u0644\u062e\u0635\u0645</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{doc.base_discount_amount}}</div>\n                    <div class=\"col-xs-1 col-1 second\">\u062c\u0645</div>\n                    <div class=\"col-xs-3 col-3 head head-col-border\"> \u0642\u064a\u0645\u0629 \u0627\u0644\u0648\u062d\u062f\u0629 \u0627\u0644\u062a\u0642\u0631\u064a\u0628\u064a\u0629 \u0628\u0639\u062f \u0627\u0644\u062e\u0635\u0645</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{doc.rounded_total}} </div>\n                    <div class=\"col-xs-1 col-1 second\">\u062c\u0645</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0641\u0642\u0637 \u0648\u0642\u062f\u0631\u0647</div>\n                    <div class=\"col-xs-8 col-8 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div> \n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u062f\u0641\u0639\u0629 \u0627\u0644\u062d\u062c\u0632</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{doc.payment_terms_template}}</div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0641\u0642\u0637 \u0648\u0642\u062f\u0631\u0647</div>\n                    <div class=\"col-xs-4 col-4 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n            </div>\n          {% endif %}        \n          {% endfor %}\n          {% for x in doc.payment_schedule%}\n                  {% if x.idx == 1 %}\n\n            <p class=\"mt-3 sections\"> \u0634\u0631\u0648\u0637 \u0627\u0644\u062a\u0639\u0627\u0642\u062f</p>\n            <div class=\"container-fluid\">\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0627\u0644\u062f\u0641\u0639\u0629 \u0627\u0644\u0645\u0642\u062f\u0645\u0629</div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\"> {{x.payment_term}} </div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0642\u064a\u0645\u0629 \u0627\u0644\u062f\u0641\u0639\u0629 \u0627\u0644\u0645\u0642\u062f\u0645\u0629</div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">{{x.outstanding}}</div>\n                    <div class=\"col-xs-2 col-2 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0639\u062f\u062f \u0633\u0646\u0648\u0627\u062a \u0627\u0644\u062a\u0642\u0633\u064a\u0637 </div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\">\u0633\u0646\u0648\u0627\u062a</div>\n                </div>\n                 <div class=\"row mt-3\">\n                    <div class=\"col-xs-3 col-3 head head-col-border\"> \u0627\u0644\u062f\u0641\u0639\u0629 \u0627\u0644\u0645\u062a\u0628\u0642\u064a\u0629 \u0644\u0625\u0633\u062a\u0643\u0645\u0627\u0644 \u0627\u0644\u062f\u0641\u0639\u0629 \u0627\u0644\u0645\u0642\u062f\u0645\u0629</div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">test</div>\n                    <div class=\"col-xs-2 col-2 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                     <div class=\"col-xs-3 col-3 head head-col-border\"> \u0623\u062e\u0631 \u0645\u0648\u0639\u062f \u0644\u0625\u0633\u062a\u0643\u0645\u0627\u0644 \u0627\u0644\u062f\u0641\u0639\u0629 \u0627\u0644\u0645\u062a\u0628\u0642\u064a\u0629 </div>\n                    <div class=\"col-xs-2 col-2 second second-col-border\">{{x.due_date}}</div>\n                </div>\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-4 col-4 head head-col-border\">   \u0646\u0633\u0628\u0629 \u0648\u062f\u064a\u0639\u0629 \u0627\u0644\u0635\u064a\u0627\u0646\u0629 \u0645\u0646 \u0623\u0635\u0644 \u0642\u064a\u0645\u0629 \u0627\u0644\u0648\u062d\u062f\u0629  \u0642\u0628\u0644 \u0627\u0644\u062e\u0635\u0645 \u062f\u0648\u0646 \u0627\u0644\u062c\u0631\u0627\u062c </div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">test</div>\n                    <div class=\"col-xs-2 col-2 second\">\u0645\u0646 \u0625\u062c\u0645\u0627\u0644\u0649 \u0642\u064a\u0645\u0629 \u0627\u0644\u0648\u062d\u062f\u0629</div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0642\u064a\u0645\u0629 \u062f\u0641\u0639\u0629 \u0627\u0644\u0635\u064a\u0627\u0646\u0629 </div>\n                    <div class=\"col-xs-1 col-1 second second-col-border\">{{doc.maintenance_payment}}</div>\n                    <div class=\"col-xs-2 col-2 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div> \n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0641\u0642\u0637 \u0648\u0642\u062f\u0631\u0647</div>\n                    <div class=\"col-xs-4 col-4 second second-col-border\">test</div>\n                    <div class=\"col-xs-1 col-1 second\"> \u062c\u0646\u064a\u0647 \u0645\u0635\u0631\u0649</div>\n                </div>\n                          {% endif %}        \n                          {% endfor %}\n            </div>\n            <p class=\"mt-3 sections\"> \u0645\u0644\u0627\u062d\u0638\u0627\u062a \u0623\u062e\u0631\u0649</p>\n            <table class=\"table table-bordered\">\n\t            <tbody>\n\t\t            <tr>\n\t\t                <td style=\"width:6%;\">*</td>\n            \t\t\t<td style=\"width:94%;\"></td>\n\t            \t</tr>\n\t\t            <tr>\n            \t\t\t<td style=\"width:6%;\">*</td>\n            \t\t\t<td style=\"width:94%;\"></td>\n\t\t            </tr>\n\t            </tbody>\n            </table>\n            <p class=\"mt-3 sections\"> \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u0639\u0645\u064a\u0644</p>\n            <div class=\"container-fluid\">\n                <div class=\"row mt-3\">\n                    <div class=\"col-xs-1 col-1 head head-col-border\"> \u0627\u0644\u0627\u0633\u0645</div>\n                    <div class=\"col-xs-4 col-4 second second-col-border\"> {{ customer.customer_name or '' }} </div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u062c\u0648\u0627\u0632 \u0633\u0641\u0631</div>\n                    <div class=\"col-xs-2 col-2 head head-col-border\"> \u0631\u0642\u0645 \u0642\u0648\u0645\u0649</div>\n                    <div class=\"col-xs-3 col-3 second second-col-bord